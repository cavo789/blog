{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "search.exclude": {
    "**/node_modules": true
  },
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    ".mdx": "Markdown"
  },
  "files.exclude": {
    "**/node_modules/**": true
  },
  "files.autoSave": "onFocusChange",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#8be959",
    "activityBar.background": "#8be959",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#5689e9",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#8be959",
    "statusBar.background": "#6ce32c",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#55c21a",
    "statusBarItem.remoteBackground": "#6ce32c",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#6ce32c",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#6ce32c99",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.remoteColor": "#6ce32c",
  "markdownlint.config": {
    "MD010": false,
    "MD033": false,
    "MD034": false
  },
  "[Makefile]": {
    "editor.foldingStrategy": "indentation",
    "editor.foldingRegions": {
      "start": "^# region",
      "end": "^# endregion"
    }
  }
}
