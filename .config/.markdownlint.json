{
    "comments": [
        "See rules at https://github.com/DavidAnson/markdownlint/blob/master/doc/Rules.md",
        "or https://github.com/markdownlint/markdownlint/blob/master/docs/RULES.md too"
    ],
    "whitespace": false,
    "header-increment": true,
    "first-header-h1": true,
    "header-style": {
        "style": "atx"
    },
    "ul-style": {
        "style": "consistent"
    },
    "list-indent": true,
    "ul-start-left": true,
    "ul-indent": {
        "indent": 2
    },
    "no-trailing-spaces": {
        "br_spaces": 0
    },
    "no-hard-tabs": {
        "code_blocks": true
    },
    "no-reversed-links": true,
    "no-multiple-blanks": {
        "maximum": 1
    },
    "line-length": false,
    "no-missing-space-atx": true,
    "no-multiple-space-atx": true,
    "blanks-around-headers": false,
    "header-start-left": true,
    "single-h1": true,
    "no-trailing-punctuation": {
        "punctuation": ".,:!?"
    },
    "no-multiple-space-blockquote": true,
    "MD029": {
        "style": "ordered"
    },
    "MD030": {
        "ul_single": 1,
        "ol_single": 1,
        "ul_multi": 1,
        "ol_multi": 1
    },
    "blanks-around-fences": true,
    "blanks-around-lists": false,
    "no-inline-html": {
        "allowed_elements": [
            "kbd"
        ]
    },
    "comment_MD034": "MD034: No bare URLs used",
    "no-bare-urls": true,
    "hr-style": {
        "style": "consistent"
    },
    "no-emphasis-as-header": true,
    "no-space-in-emphasis": true,
    "no-space-in-code": true,
    "no-space-in-links": true,
    "fenced-code-language": true,
    "first-line-h1": true,
    "no-empty-links": true,
    "MD025": {
        "front_matter_title":""
    },
    "MD044": {
        "names": [
            "Crescendo",
            "Federal Data Manager",
            "Visual Studio Code"
        ]
    },
    "no-alt-text": true,
    "MD046": {
        "style": "consistent"
    },
    "single-trailing-newline": true
}